{
  "name": "markdown",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_any"
          }
        }
      ]
    },
    "_any": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "paragraph"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[\\w\\s]+"
    },
    "bold": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "**"
              },
              {
                "type": "SYMBOL",
                "name": "text"
              },
              {
                "type": "STRING",
                "value": "**"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "__"
              },
              {
                "type": "SYMBOL",
                "name": "text"
              },
              {
                "type": "STRING",
                "value": "__"
              }
            ]
          }
        ]
      }
    },
    "paragraph": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bold"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

